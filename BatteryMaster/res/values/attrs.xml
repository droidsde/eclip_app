<?xml version="1.0" encoding="utf-8"?>
<resources>
     <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="wbarLength" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MyDigitalTimeDisplay">
        <attr name="digital_size" format="dimension" />
        <attr name="unit_size" format="dimension" />
        <attr name="digital_margin_right" format="dimension" />
        <attr name="unit_margin_right" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MybatteryGraph">
        <attr name="scale">
            <enum name="small" value="0" />
            <enum name="normal" value="1" />
        </attr>
        <attr name="useBoard" format="boolean" />
        <attr name="orientation">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="0" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MainTitle">
        <attr name="title_text" format="string" />
        <attr name="title_right_button_icon" format="reference" />
        <attr name="title_left_button_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="Digital">
        <attr name="digital_color">
            <enum name="blue" value="0" />
            <enum name="green" value="1" />
        </attr>
    </declare-styleable>
	<!-- Style of the underline indicator. -->
    <attr name="vpiUnderlinePageIndicatorStyle" format="reference"/>
    
   
</resources>